{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon src/server/main.ts -w src/server",
    "start": "NODE_ENV=production ts-node src/server/main.ts",
    "build": "vite build",
    "prepare": "husky install",
    "client": "vite",
    "format": "prettier --write src/"

  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@types/axios": "^0.14.0",
    "@types/express-fileupload": "^1.4.1",
    "@types/styled-components": "^5.1.26",
    "ai-taxonomist": "^0.7.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "^9.22.0",
    "form-data": "^4.0.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^3.3.1",
    "picocolors": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.0",
    "styled-components": "^5.3.10",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.7",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "vite": "^4.0.4"
  },
  "lint-staged": {
    "*.css": [
      "stylelint",
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
